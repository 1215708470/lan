

/*图片区域*/
div.main{
	width: 100%;
    margin: 0px auto;
    height: 376px;
    background: url(../img/login/20180214110535.png) no-repeat;
    background-size: 100%;
	min-width: 1200px;
	padding-left: 60px;
}
.main > div.pic{
	width:508px;
	margin:50px 50px 0px 0px;
}
div.userform{
	background: rgba(250,250,250,0.4);
    width: 323PX;
    height: 261PX;
    border: 1px solid #7b7979;
    margin: 50px 0px;
    border-radius: 3px;
    color: #fff;
	position: absolute;
    right: 200px;
}
div.userform > p.userform_hd{
	margin-top: 20px;
	font-size: 14px;
    font-weight: bold;
    padding-left: 40px;
    height: 30px;
    line-height: 30px;
}
.login_uame,.login_upwd{
	display: inline-block;
	width: 88px;
	height: 32px;
    padding-right: 15px;
    text-align: right;
}
#txtName,#txtPwd{
	height: 24px;
    line-height: 24px;
    width: 180px;
    border: 1px solid #c8c8c8;
    padding-left: 5px;
}
p.aoid_login{
	margin-left: 106px;
	margin-top:0px;
}
.login_btn{
	background: url(../img/login/icon11.png) 0 -100px no-repeat;
    border: 0 none;
    cursor: pointer;
    display: inline-block;
    font-size: 0;
    height: 27px;
    margin-left: 110px;
    line-height: 100px;
    margin-right: 6px;
    overflow: hidden;
    vertical-align: middle;
    width: 86px;
}
p.but_box{
    margin:0px;
}
div.note_landing{
    width:279px;
    height:30px;
    margin:5px auto;
    border-top:0.5px solid #9c9999;
}
div.note_landing>p{
    margin:3px;
    text-align: center;
}
.note_landing>p>a{
    display: inline-block;
    height: 17px;
    margin-top: 6px;
    line-height: 18px;
    vertical-align: top;
    color:#fff;
}
.phoneBg{
    display: inline-block;
    width:16px;
    height:18px;
    margin-top:5px;
    background: url(../img/login/icon11.png) -326px -196px;
}
.login_help{
	display:inline-block;
	width:12px;
	height:12px;
	background: url(../img/login/icon11.png) -325px -367px;
}
.warm_prompt {
    height: 19px;
    width: 50%;
    text-align: center;
    color: red;
    margin: 20px auto;
}
